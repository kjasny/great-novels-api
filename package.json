{
  "name": "great-novels-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "env-cmd nodemon index.js",
    "dev:migrate:up": "env-cmd sequelize db:migrate --env development --config ./configs/sequelize.js",
    "dev:migrate:undo": "env-cmd sequelize  db:migrate:undo --env development --config ./configs/sequelize.js",
    "dev:migrate:destroy": "env-cmd sequelize  db:migrate:undo:all --env development --config ./configs/sequelize.js",
    "lint": "./node_modules/.bin/eslint --format codeframe .",
    "lint:fix": "./node_modules/.bin/eslint --fix --format codeframe ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackeducation/great-novels-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stackeducation/great-novels-api/issues"
  },
  "homepage": "https://github.com/stackeducation/great-novels-api#readme",
  "dependencies": {
    "express": "^4.17.1",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.6"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^6.6.1"
  }
}
